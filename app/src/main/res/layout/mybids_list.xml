<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@drawable/list_selector"
    android:layout_height="wrap_content"

    android:layout_marginLeft="16dp"
    android:layout_marginRight="16dp">
    <ImageView

        android:id="@+id/iv_mybids"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/instagram"
        android:background="@drawable/round_pic"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="10dp"/>


    <mypocketvakil.example.com.score.custom.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_mybids_title"
        android:text="@string/title"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:paddingLeft="10dp"
        android:layout_toRightOf="@+id/iv_mybids"
        android:layout_toEndOf="@+id/iv_mybids"
        android:paddingRight="10dp"
        android:layout_marginTop="10dp"
        app:font="@string/OpenSans_Reguler"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        />
    <mypocketvakil.example.com.score.custom.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@android:color/black"


        android:layout_below="@+id/tv_mybids_title"
        android:paddingLeft="10dp"
        android:layout_marginLeft="10dp"


        android:layout_toRightOf="@+id/iv_mybids"
        android:layout_toEndOf="@+id/iv_mybids"


        android:layout_marginTop="5dp"

        android:text="@string/rupee_sign"
        android:id="@+id/tv_mybid_rupeesign1"
        app:font="@string/OpenSans_Reguler"/>
    <mypocketvakil.example.com.score.custom.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@android:color/holo_blue_dark"


        android:layout_toRightOf="@+id/tv_mybid_rupeesign1"
        android:layout_alignBottom="@+id/tv_mybid_rupeesign1"
        android:layout_marginTop="5dp"

        android:text="@string/min"
        android:id="@+id/tv_mybidbid_min"
        app:font="@string/OpenSans_Reguler"/>
    <mypocketvakil.example.com.score.custom.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@color/AppColor"


        android:layout_toRightOf="@+id/tv_mybidbid_min"
        android:layout_alignBottom="@+id/tv_mybidbid_min"


        android:layout_marginTop="5dp"
        android:layout_marginLeft="5dp"
        android:text="@string/hypen"
        android:id="@+id/tv_mybid_text7"
        app:font="@string/OpenSans_Reguler"/>
    <mypocketvakil.example.com.score.custom.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@android:color/black"


        android:layout_toRightOf="@+id/tv_mybid_text7"
        android:layout_alignBottom="@+id/tv_mybid_text7"

        android:layout_marginLeft="5dp"

        android:layout_marginTop="5dp"
        android:text="@string/rupee_sign"
        android:id="@+id/tv_mybid_rupeesign2"
        app:font="@string/OpenSans_Reguler"/>
    <mypocketvakil.example.com.score.custom.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@android:color/holo_red_dark"
        android:layout_toRightOf="@+id/tv_mybid_rupeesign2"
        android:layout_alignBottom="@+id/tv_mybid_rupeesign2"
        android:layout_marginTop="5dp"
        android:text="@string/max"
        android:id="@+id/tv_mybid_max"
        app:font="@string/OpenSans_Reguler"/>
    <mypocketvakil.example.com.score.custom.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:textColor="#22a697"
        android:layout_alignBottom="@+id/tv_mybids_title"
        android:text="@string/status"
        android:id="@+id/tv_mybid_status"
        app:font="@string/OpenSans_Reguler"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="10dp"
         />
    <mypocketvakil.example.com.score.custom.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textColor="@android:color/black"


        android:layout_below="@+id/tv_mybid_status"

        android:layout_alignStart="@+id/tv_mybid_status"


        android:layout_toRightOf="@+id/tv_mybid_max"
        android:layout_toEndOf="@+id/tv_mybid_max"


        android:layout_marginTop="5dp"
        android:layout_alignBottom="@+id/tv_mybid_max"

        android:text="@string/rupee_sign"
        android:id="@+id/tv_mybid_rupeesign3"
        app:font="@string/OpenSans_Reguler"/>
    <mypocketvakil.example.com.score.custom.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_mybid_budget"
        android:text="@string/money"
        android:textColor="@color/purple"
        android:textSize="15sp"
        android:paddingLeft="3dp"
        android:layout_toRightOf="@+id/tv_mybid_rupeesign3"
        android:layout_toEndOf="@+id/tv_mybid_rupeesign3"
        android:paddingRight="10dp"
        android:layout_below="@+id/tv_mybid_status"

        app:font="@string/OpenSans_Reguler"
        android:layout_alignTop="@+id/tv_mybid_rupeesign3"
        />




</RelativeLayout>